<p id="notice"><%= notice %></p>

<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">
      Registered Application</h3>
    </div>
    
    <table class="table table-striped table-bordered table-hover">
      <thead>
      </thead>

      <tbody>

        <tr>
          <td> Name: </td>
          <td> <%= @registered_application.name %> </td>
        </tr>
        <tr>
          <td> ID: </td>
          <td> <%= @registered_application.id %> </td>
        </tr>
        <tr>
          <td> URL: </td>
          <td> <%= @registered_application.url %> </td>
        </tr>
      </tbody>
    </table>
</div>
<%= link_to 'Edit', edit_registered_application_path(@registered_application) %> |
<%= link_to 'Back', registered_applications_path %>
